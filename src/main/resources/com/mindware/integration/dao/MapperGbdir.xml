<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mindware.integration.dao.MapperGbdir">


    <select id="getById" resultType="com.mindware.integration.entity.Gbdir" parameterType="map">
        <![CDATA[
        select *
        from gbdir
        where gbdircage = #{gbdircage}
        ]]>
    </select>


</mapper>