<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mindware.integration.dao.MapperGbdac">


    <select id="getById" resultType="com.mindware.integration.entity.Gbdac" parameterType="map">
        <![CDATA[
        select *
        from gbdac
        where gbdaccage = #{gbdaccage}
        ]]>
    </select>


</mapper>